<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.angelone.apiframework</groupId>
  <artifactId>apiframework</artifactId>
  <name>apiframework</name>
  <version>0.0.1</version>
  <description>creating framework for api</description>
  <build>
    <finalName>apiframework</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>3.0.0-M5</version>
        <configuration>
          <reuseForks>false</reuseForks>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>com.angelone.api.BaseTest</mainClass>
                </transformer>
              </transformers>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                  </excludes>
                </filter>
              </filters>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <properties>
    <noexception.version>1.7.1</noexception.version>
    <lombok.version>1.18.20</lombok.version>
    <junit.jupiter.version>5.8.2</junit.jupiter.version>
    <joda.version>2.10.13</joda.version>
    <assertjcore.version>3.21.0</assertjcore.version>
    <extentreport.version>5.0.8</extentreport.version>
    <reflection.version>0.10.2</reflection.version>
    <maven.compiler.target>8</maven.compiler.target>
    <webdrivermanager.version>5.0.3</webdrivermanager.version>
    <restassured.version>5.2.0</restassured.version>
    <testng.version>7.4.0</testng.version>
    <owner.version>1.0.12</owner.version>
    <jackson.version>2.14.1</jackson.version>
    <appium.java.version>7.6.0</appium.java.version>
    <maven.compiler.source>8</maven.compiler.source>
    <selenium.version>3.141.59</selenium.version>
  </properties>
</project>

